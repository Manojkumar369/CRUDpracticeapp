<%= form_with model: contact do |form| %>
<div>
	<%= form.label :name %><br>
	<%= form.text_field :name %>
</div>

<div>
	<%= form.label :email %><br>
	<%= form.text_field :email %>
</div>
<div>
	<%= form.label :description %><br>
	<%= form.text_field :description %>
</div>		
<div>
	<%= form.label :mobile_no %><br>
	<%= form.text_field :mobile_no %>
</div>
<div>
	<%= form.submit %>
</div>
<% end %>